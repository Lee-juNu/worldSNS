<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fakeBoardMapper">

<insert id= "boardInsert" parameterType="com.team.worlds.board.Board">
	insert into boardTbl values(boardTbl_seq.nextval,#{board_parentNum},#{board_userid},
	#{board_img1},#{board_img2},#{board_img3},#{board_img4},#{board_contents},#{board_city})
</insert>

</mapper>
